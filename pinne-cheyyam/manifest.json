{
  "manifest_version": 2,
  "name": "Pinne-Cheyyam",
  "version": "1.0",
  "description": "Annoys users with quizzes, puzzles, and mini-games when they are productive.",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": ["tabs", "storage", "activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/icon.png"
  },
  "options_page": "options/options.html"
}
